{
  "expo": {
    "name": "Chessable",
    "slug": "Chessable",
    "author": "Thomas Catt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
